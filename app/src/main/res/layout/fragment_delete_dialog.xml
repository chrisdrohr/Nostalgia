<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/AccentLighter"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context="com.example.gabekeyner.nostalgia.DeleteDialogFragment">

    <android.support.v7.widget.CardView
        app:cardBackgroundColor="@color/MainColor"
        android:layout_centerHorizontal="true"
        android:layout_margin="40dp"
        android:layout_width="250dp"
        android:layout_height="150dp"
        android:id="@+id/cardViewprofile">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/dialogUserImageView"
                app:border_width="2dp"
                app:border_color="@color/AccentLighter"
                android:layout_centerInParent="true"
                app:border_overlay="true"
                android:layout_width="80dp"
                android:layout_height="80dp"
                />
            <TextView
                android:id="@+id/dialogTextView"
                android:layout_margin="10dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/dialogUserImageView"
                android:textAlignment="center"
                android:layout_width="match_parent"/>

        </RelativeLayout>

    </android.support.v7.widget.CardView>

    <com.dragankrstic.autotypetextview.AutoTypeTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:id="@+id/deleteAutoText"
        android:layout_below="@id/cardViewprofile"
        android:textAlignment="center" />

